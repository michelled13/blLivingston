import json
import re
import requests

response = requests.get('https://www.instagram.com/' + 'thebarkerloungelivingston')
json_match = re.search(r'window\._sharedData = (.*);</script>', response.text)
profile_json = json.loads(json_match.group(1))['entry_data']['ProfilePage'][0]['graphql']['user']

followers = profile_json['edge_followed_by']


print (followers)
